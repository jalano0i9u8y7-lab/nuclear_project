{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "LiquidityStressResult",
    "type": "object",
    "properties": {
        "stress_level": {
            "type": "string",
            "enum": [
                "high",
                "medium",
                "low"
            ]
        },
        "vix": {
            "type": "number"
        },
        "confidence": {
            "type": "number",
            "minimum": 0,
            "maximum": 1
        }
    },
    "required": [
        "stress_level",
        "vix",
        "confidence"
    ]
}