{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "StrategyProfileResult",
    "type": "object",
    "properties": {
        "posture": {
            "type": "string",
            "enum": [
                "aggressive",
                "defensive",
                "neutral"
            ]
        },
        "holding_period": {
            "type": "string",
            "enum": [
                "swing",
                "short",
                "position"
            ]
        },
        "confidence": {
            "type": "number",
            "minimum": 0,
            "maximum": 1
        }
    },
    "required": [
        "posture",
        "holding_period",
        "confidence"
    ]
}